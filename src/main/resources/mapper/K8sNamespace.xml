<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">


<mapper namespace="com.jd.dao.K8sNamespaceDao">

    <resultMap id="k8sNamespace" type="com.jd.model.K8sNamespace">
        <id property="id" column="id"/>
        <result property="userName" column="user_name"/>
        <result property="namespaceName" column="namespace_name"/>
    </resultMap>

    <insert id="insertNamespace" parameterType="K8sNamespace" useGeneratedKeys="true" keyProperty="id">
        INSERT INTO namespace (user_name, namespace_name) VALUES (#{userName}, #{namespaceName});
    </insert>
</mapper>